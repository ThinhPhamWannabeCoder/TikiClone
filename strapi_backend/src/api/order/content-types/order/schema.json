{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "oder_detail": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::oder-detail.oder-detail",
      "mappedBy": "order"
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address"
    },
    "store": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::store.store"
    },
    "delivery": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::delivery.delivery"
    },
    "deliveryTotalCost": {
      "type": "decimal"
    },
    "productTotalCost": {
      "type": "decimal"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Đã đặt hàng",
        "Đã nhận đơn",
        "Đang chuẩn bị hàng",
        "Đang vận chuyển",
        "Đang giao",
        "Giao hàng thành công",
        "Giao hàng thất bại"
      ],
      "default": "Đã đặt hàng"
    },
    "seller_status": {
      "type": "enumeration",
      "enum": [
        "Chưa thanh toán",
        "Đã thanh toán"
      ],
      "default": "Chưa thanh toán"
    },
    "buyer_status": {
      "type": "enumeration",
      "enum": [
        "Đã thanh toán",
        "Chưa thanh toán"
      ],
      "default": "Chưa thanh toán"
    }
  }
}
