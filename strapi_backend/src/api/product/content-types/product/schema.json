{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "price": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "Inventory": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "integer",
      "required": true,
      "default": 0
    },
    "product_sub_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-sub-category.product-sub-category",
      "inversedBy": "products"
    },
    "store": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::store.store",
      "inversedBy": "products"
    },
    "product_images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "primary_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "product_color": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-color.product-color",
      "inversedBy": "products"
    },
    "weight": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal"
    },
    "length": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal"
    },
    "depth": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal"
    },
    "width": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "decimal"
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "json"
    },
    "product_material": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-material.product-material",
      "inversedBy": "products"
    },
    "weight_unit": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::weight-unit.weight-unit",
      "inversedBy": "products"
    },
    "length_unit": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::length-unit.length-unit",
      "inversedBy": "products"
    },
    "depth_unit": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::depth-unit.depth-unit",
      "inversedBy": "products"
    },
    "width_unit": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::width-unit.width-unit",
      "inversedBy": "products"
    }
  }
}
